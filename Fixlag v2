--------------------------------------------------
-- CLEANUP FUNCTION
--------------------------------------------------
local function cleanup()
    print("üßπ Cleaning up Minh Thu·∫≠n Hub...")
    
    for _, connection in pairs(CONNECTIONS) do
        if connection then
            connection:Disconnect()
        end
    end
    
    for _, object in pairs(OBJECTS) do
        if typeof(object) == "Instance" and object then
            object:Destroy()
        elseif typeof(object) == "table" then
            for _, obj in pairs(object) do
                if typeof(obj) == "Instance" and obj then obj:Destroy() end
                if typeof(obj) == "table" then
                    for _, subobj in ipairs(obj) do
                        if subobj then subobj:Destroy() end
                    end
                end
            end
        end
    end
    
    for key in pairs(STATE) do
        STATE[key] = false
    end
    
    print("‚úÖ Cleanup complete")
end

game:BindToClose(cleanup)

-- Anti-delete
gui.Parent = game.CoreGui

print("\n‚úÖ Minh Thu·∫≠n Hub v2.2 ready! 10 new features, 2 new tabs, mobile optimized, errors fixed.")
