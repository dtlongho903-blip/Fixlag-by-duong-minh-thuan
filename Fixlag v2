local function toggleGodMode()
    STATE.GOD_MODE = not STATE.GOD_MODE
    
    local function applyGod()
        local character = player.Character
        if not character then return end
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if not humanoid then return end
        if STATE.GOD_MODE then
            humanoid.MaxHealth = math.huge
            humanoid.Health = math.huge
            CONNECTIONS.GodMode = humanoid.HealthChanged:Connect(function(health)
                if health < math.huge then
                    humanoid.Health = math.huge
                end
            end)
        else
            if CONNECTIONS.GodMode then
                CONNECTIONS.GodMode:Disconnect()
                CONNECTIONS.GodMode = nil
            end
            humanoid.MaxHealth = 100
            humanoid.Health = 100
        end
    end
    
    applyGod()
    print(STATE.GOD_MODE and "‚úÖ God Mode ENABLED" or "‚úÖ God Mode DISABLED")
end

local function teleportToPlayer(targetPlayer)
    local character = player.Character
    if not character then return end
    local root = character:FindFirstChild("HumanoidRootPart")
    if not root then return end
    
    local targetChar = targetPlayer.Character
    if not targetChar then return end
    local targetRoot = targetChar:FindFirstChild("HumanoidRootPart")
    if not targetRoot then return end
    
    root.CFrame = targetRoot.CFrame + Vector3.new(0, 5, 0)
    print("‚úÖ Teleported to " .. targetPlayer.Name)
end

local function toggleItemESP()
    STATE.ITEM_ESP = not STATE.ITEM_ESP
    
    if STATE.ITEM_ESP then
        OBJECTS.ItemESP = {}
        
        local function createItemESP(item)
            if not item:IsA("BasePart") or item.Parent == player.Character then return end
            
            local highlight = Instance.new("Highlight")
            highlight.Name = "ItemESP"
            highlight.FillColor = Color3.fromRGB(0, 255, 0)
            highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
            highlight.FillTransparency = 0.5
            highlight.OutlineTransparency = 0
            highlight.Parent = item
            
            local billboard = Instance.new("BillboardGui")
            billboard.Name = "ItemTag"
            billboard.Size = UDim2.new(0, 100, 0, 30)
            billboard.StudsOffset = Vector3.new(0, 2, 0)
            billboard.AlwaysOnTop = true
            billboard.Parent = item
            
            local label = Instance.new("TextLabel")
            label.Size = UDim2.new(1, 0, 1, 0)
            label.Text = item.Name
            label.TextColor3 = Color3.fromRGB(0, 255, 0)
            label.BackgroundTransparency = 1
            label.Parent = billboard
            
            table.insert(OBJECTS.ItemESP, {highlight, billboard})
        end
        
        for _, item in ipairs(workspace:GetDescendants()) do
            if item:IsA("BasePart") and item.Parent.Name == "Items" then -- Assume items in 'Items' folder, adjust as needed
                createItemESP(item)
            end
        end
        
        CONNECTIONS.ItemAdded = workspace.DescendantAdded:Connect(function(item)
            if STATE.ITEM_ESP and item:IsA("BasePart") and item.Parent.Name == "Items" then
                createItemESP(item)
            end
        end)
        
        print("‚úÖ Item ESP ENABLED")
    else
        if CONNECTIONS.ItemAdded then
            CONNECTIONS.ItemAdded:Disconnect()
            CONNECTIONS.ItemAdded = nil
        end
        
        for _, objs in ipairs(OBJECTS.ItemESP or {}) do
            for _, obj in ipairs(objs) do
                if obj then obj:Destroy() end
            end
        end
        OBJECTS.ItemESP = {}
        print("‚úÖ Item ESP DISABLED")
    end
end

local function toggleReachHack()
    STATE.REACH_HACK = not STATE.REACH_HACK
    
    local function applyReach()
        local character = player.Character
        if not character then return end
        local tool = character:FindFirstChildOfClass("Tool")
        if tool then
            local handle = tool:FindFirstChild("Handle")
            if handle then
                handle.Size = STATE.REACH_HACK and Vector3.new(SETTINGS.REACH_DISTANCE, 1, 1) or Vector3.new(1, 1, 1)
            end
        end
    end
    
    applyReach()
    CONNECTIONS.ReachToolAdded = player.Character.ChildAdded:Connect(function(child)
        if STATE.REACH_HACK and child:IsA("Tool") then
            applyReach()
        end
    end)
    print(STATE.REACH_HACK and "‚úÖ Reach Hack ENABLED" or "‚úÖ Reach Hack DISABLED")
end

local function toggleAutoRespawn()
    STATE.AUTO_RESPAWN = not STATE.AUTO_RESPAWN
    
    if STATE.AUTO_RESPAWN then
        CONNECTIONS.AutoRespawn = player.CharacterAdded:Connect(function()
            print("‚úÖ Auto Respawn triggered")
        end)
        local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.Died:Connect(function()
                game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("Respawn"):FireServer() -- Assume respawn remote, adjust
            end)
        end
        print("‚úÖ Auto Respawn ENABLED")
    else
        if CONNECTIONS.AutoRespawn then
            CONNECTIONS.AutoRespawn:Disconnect()
            CONNECTIONS.AutoRespawn = nil
        end
        print("‚úÖ Auto Respawn DISABLED")
    end
end

local function toggleChatSpam()
    STATE.CHAT_SPAM = not STATE.CHAT_SPAM
    
    if STATE.CHAT_SPAM then
        CONNECTIONS.ChatSpam = RunService.Heartbeat:Connect(function()
            if STATE.CHAT_SPAM then
                ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(SETTINGS.CHAT_SPAM_MESSAGE, "All")
                task.wait(SETTINGS.CHAT_SPAM_DELAY)
            end
        end)
        print("‚úÖ Chat Spam ENABLED")
    else
        if CONNECTIONS.ChatSpam then
            CONNECTIONS.ChatSpam:Disconnect()
            CONNECTIONS.ChatSpam = nil
        end
        print("‚úÖ Chat Spam DISABLED")
    end
end

local function startServerHopper()
    STATE.SERVER_HOPPER = not STATE.SERVER_HOPPER
    
    if STATE.SERVER_HOPPER then
        CONNECTIONS.ServerHopper = RunService.Heartbeat:Connect(function()
            if STATE.SERVER_HOPPER then
                local success, result = pcall(function()
                    local url = "https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?limit=100"
                    local response = game:HttpGet(url)
                    local data = HttpService:JSONDecode(response)
                    local randomServer = data.data[math.random(1, #data.data)]
                    TeleportService:TeleportToPlaceInstance(game.PlaceId, randomServer.id)
                end)
                task.wait(60) -- Hop every minute
            end
        end)
        print("‚úÖ Server Hopper ENABLED")
    else
        if CONNECTIONS.ServerHopper then
            CONNECTIONS.ServerHopper:Disconnect()
            CONNECTIONS.ServerHopper = nil
        end
        print("‚úÖ Server Hopper DISABLED")
    end
end

local function toggleFpsUnlocker()
    STATE.FPS_UNLOCKER = not STATE.FPS_UNLOCKER
    
    if STATE.FPS_UNLOCKER then
        setfpscap(SETTINGS.FPS_CAP)
        print("‚úÖ FPS Unlocker ENABLED: " .. SETTINGS.FPS_CAP)
    else
        setfpscap(60)
        print("‚úÖ FPS Unlocker DISABLED")
    end
end

local function toggleLowGravity()
    STATE.LOW_GRAVITY = not STATE.LOW_GRAVITY
    
    if STATE.LOW_GRAVITY then
        workspace.Gravity = workspace.Gravity * SETTINGS.GRAVITY_MULTIPLIER
        print("‚úÖ Low Gravity ENABLED")
    else
        workspace.Gravity = 196.2 -- Default
        print("‚úÖ Low Gravity DISABLED")
    end
end

local function toggleInvisible()
    STATE.INVISIBLE = not STATE.INVISIBLE
    
    local function applyInvisible()
        local character = player.Character
        if not character then return end
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
                part.Transparency = STATE.INVISIBLE and 1 or 0
            end
        end
    end
    
    applyInvisible()
    print(STATE.INVISIBLE and "‚úÖ Invisible ENABLED" or "‚úÖ Invisible DISABLED")
end

