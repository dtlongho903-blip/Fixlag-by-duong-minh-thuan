-- Create tabs
local tabs = {}
local tabContents = {}

local function createTab(name, position)
    local tabBtn = Instance.new("TextButton")
    tabBtn.Size = UDim2.new(0.18, 0, 1, 0) -- Adjusted for more tabs
    tabBtn.Text = name
    tabBtn.TextScaled = true
    tabBtn.Font = Enum.Font.GothamBold
    tabBtn.TextColor3 = Color3.new(1, 1, 1)
    tabBtn.AutoButtonColor = false
    tabBtn.Parent = tabFrame
    Instance.new("UICorner", tabBtn)
    
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Size = UDim2.new(1, 0, 0.68, 0)
    scrollFrame.Position = UDim2.new(0, 0, 0.22, 0)
    scrollFrame.CanvasSize = UDim2.new(0, 0, 4, 0) -- Larger canvas
    scrollFrame.ScrollBarThickness = 8 -- Thicker for mobile
    scrollFrame.BackgroundTransparency = 1
    scrollFrame.Visible = false
    scrollFrame.Parent = main
    
    local listLayout = Instance.new("UIListLayout", scrollFrame)
    listLayout.Padding = UDim.new(0, 15) -- More spacing for mobile
    listLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    
    tabs[name] = {button = tabBtn, frame = scrollFrame}
    
    return scrollFrame
end

-- Create all tabs including 2 new: VISUALS and UTILITY
local mainScroll = createTab("MAIN", 0)
local hackScroll = createTab("HACK", 0)
local serverScroll = createTab("SERVER", 0)
local settingsScroll = createTab("SETTINGS", 0)
local visualsScroll = createTab("VISUALS", 0)
local utilityScroll = createTab("UTILITY", 0)

-- Initialize tabs
for name, tab in pairs(tabs) do
    if name == "MAIN" then
        tab.button.BackgroundColor3 = Color3.fromRGB(120, 70, 180)
        tab.frame.Visible = true
    else
        tab.button.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
        tab.frame.Visible = false
    end
end

-- Tab switching
for name, tab in pairs(tabs) do
    tab.button.MouseButton1Click:Connect(function()
        for otherName, otherTab in pairs(tabs) do
            otherTab.frame.Visible = false
            otherTab.button.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
        end
        tab.frame.Visible = true
        tab.button.BackgroundColor3 = Color3.fromRGB(120, 70, 180)
    end)
end

--------------------------------------------------
-- BUTTON CREATOR (LARGER BUTTONS FOR MOBILE)
--------------------------------------------------
local function createButton(parent, text, callback, color)
    local b = Instance.new("TextButton")
    b.Size = UDim2.new(0.9, 0, 0, 80) -- Larger height for touch
    b.Text = text
    b.TextWrapped = true
    b.TextScaled = true
    b.Font = Enum.Font.GothamBold
    b.BackgroundColor3 = color or Color3.fromRGB(90, 40, 140)
    b.TextColor3 = Color3.new(1, 1, 1)
    b.AutoButtonColor = true
    b.Parent = parent
    
    local corner = Instance.new("UICorner", b)
    corner.CornerRadius = UDim.new(0, 12)
    
    b.MouseButton1Click:Connect(callback)
    
    return b
end

local function createSlider(parent, name, min, max, default, callback)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0.9, 0, 0, 50)
    frame.BackgroundTransparency = 1
    frame.Parent = parent
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 0.5, 0)
    label.Text = name
    label.Font = Enum.Font.GothamBold
    label.TextColor3 = Color3.new(1,1,1)
    label.BackgroundTransparency = 1
    label.Parent = frame
    
    local slider = Instance.new("TextButton")
    slider.Size = UDim2.new(1, 0, 0.5, 0)
    slider.Text = tostring(default)
    slider.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
    slider.Parent = frame
    Instance.new("UICorner", slider)
    
    -- Simple slider logic (click to increase/decrease)
    local value = default
    slider.MouseButton1Click:Connect(function()
        value = value + 1
        if value > max then value = min end
        slider.Text = tostring(value)
        callback(value)
    end)
    
    return frame
end

