-- T·∫°o m√†n h√¨nh credit
local creditGui = Instance.new("ScreenGui", game.CoreGui)
creditGui.Name = "CreditGUI"
creditGui.ResetOnSpawn = false

local creditFrame = Instance.new("Frame", creditGui)
creditFrame.Size = UDim2.new(1, 0, 1, 0)
creditFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
creditFrame.BackgroundTransparency = 0
creditFrame.ZIndex = 999

local creditText = Instance.new("TextLabel", creditFrame)
creditText.Size = UDim2.new(1, 0, 1, 0)
creditText.Text = "Credit: D∆∞∆°ng Minh Thu·∫≠n\nMINH THU·∫¨N GUI FIXLAG"
creditText.TextColor3 = Color3.fromRGB(255, 255, 255)
creditText.BackgroundTransparency = 1
creditText.Font = Enum.Font.GothamBold
creditText.TextSize = 36
creditText.TextScaled = false
creditText.ZIndex = 1000

-- ·∫®n credit sau 2 gi√¢y
task.delay(2, function()
    local tween = TweenService:Create(creditFrame, TweenInfo.new(1), {BackgroundTransparency = 1})
    local textTween = TweenService:Create(creditText, TweenInfo.new(1), {TextTransparency = 1})
    tween:Play()
    textTween:Play()
    
    tween.Completed:Connect(function()
        creditGui:Destroy()
        -- Load Rayfield v√† t·∫°o GUI ch√≠nh
        createMainGUI()
    end)
end)

--------------------------------------------------
-- STATE
--------------------------------------------------
local STATE = {
    FX = false,
    GRAY = false,
    SOUND = false,
    POST = false,
    SHADOW = false,
}

--------------------------------------------------
-- CORE SAFE FUNCTIONS
--------------------------------------------------
local function clearFX()
    for _,v in ipairs(workspace:GetDescendants()) do
        if v:IsA("ParticleEmitter") or v:IsA("Trail")
        or v:IsA("Beam") or v:IsA("Fire")
        or v:IsA("Smoke") or v:IsA("Sparkles") then
            v.Enabled = false
        end
    end
end

local function grayChar(m)
    for _,v in ipairs(m:GetDescendants()) do
        if v:IsA("BasePart") then
            v.Material = Enum.Material.Plastic
            v.Color = Color3.fromRGB(135,135,135)
            v.CastShadow = false
        elseif v:IsA("Decal") or v:IsA("Texture") then
            v:Destroy()
        end
    end
end

