    local otherPlayers = Players:GetPlayers()
    table.remove(otherPlayers, table.find(otherPlayers, player))
    if #otherPlayers > 0 then
        teleportToPlayer(otherPlayers[1]) -- To first other player, can improve with dropdown
    else
        print("‚ùå No other players")
    end
end)

