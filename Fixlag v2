-- T·ª± ƒë·ªông √°p d·ª•ng m·ªôt s·ªë t·ªëi ∆∞u khi join game
task.spawn(function()
    task.wait(5) -- Ch·ªù 5 gi√¢y sau khi join game
    
    -- T·ª± ƒë·ªông t·∫Øt shadow ƒë·ªÉ tƒÉng FPS
    Lighting.GlobalShadows = false
    print("‚úÖ ƒê√£ t·ª± ƒë·ªông t·∫Øt Global Shadows")
    
    -- T·ª± ƒë·ªông t·∫Øt m·ªôt s·ªë post effect ph·ªï bi·∫øn
    for _,v in ipairs(Lighting:GetChildren()) do
        if v:IsA("BloomEffect") or v:IsA("BlurEffect") or v:IsA("ColorCorrectionEffect") then
            v.Enabled = false
        end
    end
    print("‚úÖ ƒê√£ t·ª± ƒë·ªông t·∫Øt Post Effects")
end)

-- Hook ƒë·ªÉ ch·ªëng anti-cheat detection
local originalNamecall
originalNamecall = hookmetamethod(game, "__namecall", function(self, ...)
    local method = getnamecallmethod()
    local args = {...}
    
    -- Ch·ªëng ph√°t hi·ªán khi t·∫Øt √¢m thanh
    if method == "Play" and self:IsA("Sound") then
        if STATE.SOUND then
            return nil
        end
    end
    
    return originalNamecall(self, ...)
end)

print("üõ°Ô∏è Anti-detection hooks ƒë√£ ƒë∆∞·ª£c k√≠ch ho·∫°t")
