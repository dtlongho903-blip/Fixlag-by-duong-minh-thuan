    STATE.ANTI_AFK = not STATE.ANTI_AFK
    
    if STATE.ANTI_AFK then
        CONNECTIONS.AntiAFK = player.Idled:Connect(function()
            local VirtualUser = game:GetService("VirtualUser")
            VirtualUser:CaptureController()
